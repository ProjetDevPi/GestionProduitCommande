      <Label layoutX="13.0" layoutY="157.0" prefHeight="25.0" prefWidth="103.0" text="Capacite :">
         <font>
            <Font name="Comic Sans MS Bold Italic" size="15.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="215.0" prefHeight="25.0" prefWidth="103.0" text="image :">
         <font>
            <Font name="Comic Sans MS Bold Italic" size="15.0" />
         </font>
      </Label>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="62.0" layoutY="346.0" pickOnBounds="true" preserveRatio="true">
