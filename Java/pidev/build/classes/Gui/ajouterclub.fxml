      <TextField fx:id="filechoose" disable="true" layoutX="78.0" layoutY="214.0" prefHeight="25.0" prefWidth="155.0" promptText="Image name" />
      <Button fx:id="chooser" layoutX="242.0" layoutY="214.0" mnemonicParsing="false" onAction="#image" prefHeight="18.0" prefWidth="55.0" stylesheets="@ajouterequipement.css" text="choose">
         <font>
            <Font name="Comic Sans MS Italic" size="12.0" />
