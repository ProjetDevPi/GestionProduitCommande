            <ImageView fitHeight="211.0" fitWidth="286.0">
               <image>
                  <Image url="@../images/logo-white.png" />
               </image>
            </ImageView>
            <Label text="Club Edit" textFill="#aff23b" wrapText="true">
               <font>
                  <Font name="Comic Sans MS Bold Italic" size="35.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets top="-50.0" />
         </padding>
      </VBox>
      <Button fx:id="add" layoutX="49.0" layoutY="280.0" mnemonicParsing="false" onAction="#Modifier" stylesheets="@Button.css" text="Modifier">
