            <Font name="Comic Sans MS Italic" size="16.0" />
         </font></Button>
      <Button fx:id="btn_cancel" layoutX="160.0" layoutY="280.0" mnemonicParsing="false" onAction="#cancel" prefHeight="35.0" prefWidth="80.0" stylesheets="@stylekarim.css" text="Cancel">
